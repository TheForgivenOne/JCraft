{
  "security": {
    "auth": {
      "selectedType": "qwen-oauth"
    }
  },
  "$version": 2,
  "ide": {
    "hasSeenNudge": true,
    "enabled": true
  },
  "ui": {
    "theme": "Qwen Dark",
    "footer": {
      "hideModelInfo": false
    },
    "showMemoryUsage": true,
    "showCitations": true
  },
  "mcpServers": {
    "context7": {
      "httpUrl": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "",
        "Accept": "application/json, text/event-stream"
      }
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["@modelcontextprotocol/server-sequential-thinking"]
    }
  },
  "general": {
    "preferredEditor": "vim"
  },
  "output": {
    "format": "json"
  },
  "tools": {
    "shell": {
      "showColor": false,
      "enableInteractiveShell": true
    },
    "approvalMode": "default",
    "autoAccept": false,
    "useBuiltinRipgrep": true
  }
}
